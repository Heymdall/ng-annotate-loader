  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huston007/ng-annotate-loader/issues"
  },
  "homepage": "https://github.com/huston007/ng-annotate-loader",
  "dependencies": {
    "ng-annotate": "^1.0.2",
    "source-map": "0.4.2",
    "loader-utils": "^0.2.6"
  },
  "devDependencies": {
    "babel-loader": "^5.3.2",
    "node-libs-browser": "0.5.2",
    "webpack": "^1.12.0"
  }
